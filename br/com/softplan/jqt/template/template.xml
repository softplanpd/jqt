<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>
  
  <template name="spwDoc" autoinsert="true" context="java" deleted="false" description="Softplan JavaDoc" dynamic="true" dynamicStrategy="0" enabled="true">
    /**&#13;
     * ${objetivo}
     * @precondition ${precondition}
     * @postcondition ${postcondition}#{sp_parameters}
     * @rule ${rule}#{sp_return}#{sp_throws}
     */
  </template>
    
  <template id="zfCNPJ" name="zfCNPJ" autoinsert="true" context="html_all" deleted="false" description="Input com formatação de CNPJs." enabled="true" dynamic="false">
   &lt;z:cnpj id="${id}" label="${label}" value="${value}"/&gt;
  </template>

  <template id="zfCNPJOutput" name="zfCNPJOutput" autoinsert="true" context="html_all" deleted="false" description="Output para a exibição de CNPJs formatados." enabled="true" dynamic="false">
   &lt;z:cnpj id="${id}" label="${label}" value="${value}" renderAsOutput="true"/&gt;
  </template>
  
  <template id="zfCPF" name="zfCPF" autoinsert="true" context="html_all" deleted="false" description="Input com formatação de CPFs." enabled="true" dynamic="false">
   &lt;z:cpf id="${id}" label="${label}" value="${value}"/&gt;
  </template>

  <template id="zfCPFOutput"  name="zfCPFOutput" autoinsert="true" context="html_all" deleted="false" description="Output para a exibição de CPFs formatados." enabled="true" dynamic="false">
   &lt;z:cpf id="${id}" label="${label}" value="${value}" renderAsOutput="true"/&gt;
  </template>
  
  <template id="zfDate" name="zfDate" autoinsert="true" context="html_all" deleted="false" description="Input com a máscara de data." enabled="true" dynamic="false">
   &lt;z:date id=${id} label="${label}" value="${value}"/&gt;
  </template>

  <template id="zfDateOutput" name="zfDateOutput" autoinsert="true" context="html_all" deleted="false" description="Output para a exibição de textos de datas formatadas." enabled="true" dynamic="false">
   &lt;z:date id=${id} label="${label}" value="${value}" renderAsOutput="true"/&gt;
  </template>
  
  <template id="zfEmail"  name="zfEmail" autoinsert="true" context="html_all" deleted="false" description="Input com formatação de e-mails." enabled="true" dynamic="false">
   &lt;z:email id=${id} label="${label}" value="${value}"/&gt;
  </template>

  <template id="zfEmailOutput"  name="zfEmailOutput" autoinsert="true" context="html_all" deleted="false" description="Output para a exibição de e-mails." enabled="true" dynamic="false">
   &lt;z:email id=${id} label="${label}" value="${value}" renderAsOutput="true"/&gt;
  </template>
  
  
</templates>
